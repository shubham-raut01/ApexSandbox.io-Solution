public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
 
    if((passedExam==true && passedAssignments ==true) || (passedProject==true && passedAssignments ==true) || (passedProject==true && passedExam ==true) ){
        return true;
    }
    else{
        return false;
    }
 }