public Boolean companionPlants(String plant1, String plant2) {
    if((plant1 == 'onions' && (plant2 == 'lettuce' || plant2 == 'carrots' || plant2 == 'tomatoes')) || (plant1 == 'lettuce' && plant2 == 'cucumbers') || (plant1 == 'cucumbers' && plant2 == 'lettuce') || (plant2 == 'onions' && (plant1 == 'lettuce' || plant1 == 'carrots' || plant1 == 'tomatoes'))){
       return true;
    }
    else{
        return false;
    }
}